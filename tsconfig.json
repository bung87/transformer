{
    "compilerOptions": {
        "outDir": "./dist/",
        "checkJs": false,
        "skipLibCheck": true,
        "noImplicitAny": false,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "downlevelIteration":true,
        "allowSyntheticDefaultImports": true,
        "allowJs": false
    },
    "exclude": ["node_modules/**"]
}